{
  "name": "backend-crud-test",
  "version": "1.0.0",
  "description": "RESTful API developed as part of a backend technical challenge. The application performs CRUD operations on a generic object using Node.js, Express, and MongoDB. The database runs inside a Docker container. The project follows clean architecture practices and includes Swagger documentation and unit tests with Jest.",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "start": "nodemon src/app.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hnrkDEV/backend-crud-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/hnrkDEV/backend-crud-test/issues"
  },
  "homepage": "https://github.com/hnrkDEV/backend-crud-test#readme",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.15.1",
    "nodemon": "^3.1.10",
    "swagger": "^0.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "supertest": "^7.1.1"
  }
}
